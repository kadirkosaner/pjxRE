<<silently>>
    <<set $location = "fhKitchen">>
    <<advanceTime 20>>
    <<loseStat "hunger" 50>>
    <<loseStat "thirst" 100>>
    <<run window.notifySuccess("Hunger & Thirst Restored")>>
    <<gainStat "energy" 5>>
    <<loseStat "stress" 5>>
<</silently>>

<<narrative "Eat Food">>
